<script>
    function loadFile(){
        var file = document.getElementById('input-excel').files[0];
        console.log(file)
        var reader = new FileReader();
        reader.readAsArrayBuffer(file);
        
        reader.onload = function(file) {
            var datas = [];
            var data = new Uint8Array(reader.result);
            var wb = XLSX.read(data,{type:'array'});
            var csv = XLSX.utils.sheet_to_json(wb.Sheets[wb.SheetNames[0]], {header:1});
            for(var i = 1; i < csv.length; i++){

                dataAux = []
                for(var j = 0; j < csv[i].length; j++){
                    dataAux.push(csv[i][j]);
                }
                datas.push(dataAux)

            }
            console.log(csv)
            dta = {};
            dta.document = datas;
            dta.socio = document.getElementById("organizacion").value
            dta.fecha = document.getElementById("date").value
            dta.tipo_aparicion = document.getElementById("tipo_aparicion").value
            dta.nombre = document.getElementById("nombre").value
            dta.document_blob = new Blob([data])

            $.ajax({
                type: "POST",
                url: "/executeLoadFile",
                async: false,
                dataType: 'json',
                contentType: 'application/json',
                data: JSON.stringify(dta)
            }).done(function(status){
                console.log(status)
            });
            return false

        }
    }

    ( function ( $ ) {
        "use strict";
        jQuery( '#vmap' ).vectorMap( {
            map: 'world_en',
            backgroundColor: null,
            color: '#ffffff',
            hoverOpacity: 0.7,
            selectedColor: '#1de9b6',
            enableZoom: true,
            showTooltip: true,
            values: sample_data,
            scaleColors: [ '#1de9b6', '#03a9f5' ],
            normalizeFunction: 'polynomial'
        } );
    } )( jQuery );
</script>

