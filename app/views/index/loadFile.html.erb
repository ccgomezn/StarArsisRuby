<script>
    function loadFile(){

      

        var file = document.getElementById('input-excel').files[0];
        var reader = new FileReader();
        if(document.getElementById("organizacion").value != 6){

            reader.readAsArrayBuffer(file);
            
            reader.onload = function(file) {
                var dta = {};
                dta.socio = document.getElementById("organizacion").value
                    var datas = [];
                    var data = new Uint8Array(reader.result);
                    var wb = XLSX.read(data,{type:'array'});
                    var csv = XLSX.utils.sheet_to_json(wb.Sheets[wb.SheetNames[0]], {header:1});
                    for(var i = 1; i < csv.length; i++){

                        dataAux = []
                        for(var j = 0; j < csv[i].length; j++){
                            if(csv[i][j] == null){
                            dataAux.push('');

                            }else{
                            dataAux.push(csv[i][j]);

                            }
                        }
                        datas.push(dataAux)

                    }
                    dta.document = datas;
                
                    
                
                dta.socio = document.getElementById("organizacion").value
                dta.fecha = document.getElementById("date").value
                dta.tipo_aparicion = document.getElementById("tipo_aparicion").value
                dta.nombre = document.getElementById("nombre").value
                dta.document_blob = new Blob([data])
                dta.mas_iva = document.getElementById("mas_iva").value
                dta.iva_incluido = document.getElementById("iva_incluido").value
                dta.descuento = document.getElementById("descuento").value
                dta.euro_dolar = document.getElementById("euro_dolar").value
                dta.dolar_peso = document.getElementById("dolar_peso").value
                dta.territorio = document.getElementById("territorio").options[document.getElementById("territorio").selectedIndex].value;
                $.ajax({
                    type: "POST",
                    url: "/executeLoadFile",
                    async: false,
                    dataType: 'json',
                    contentType: 'application/json',
                    data: JSON.stringify(dta)
                }).done(function(status){
                    str = "";
                    for(var i = 0; i < status.no_subido.length; i++){
                        str += status.no_subido[i] + '\n'
                    }
                    
                    document.getElementById("obr").innerText += str 
                    $('.hover_bkgr_fricc').show();           
                    
                    });
                return false

            }

        }else{
            console.log("alv")
        
           
            reader.readAsText(file);

            reader.onload = function(file) {
                console.log(event.target.result)
                var dta = {};
                var csv = event.target.result.split(/\n/);

                    dta.document = csv;
                
                console.log(csv) 
                
                dta.socio = document.getElementById("organizacion").value
                dta.fecha = document.getElementById("date").value
                dta.tipo_aparicion = document.getElementById("tipo_aparicion").value
                dta.nombre = document.getElementById("nombre").value
                dta.mas_iva = document.getElementById("mas_iva").value
                dta.iva_incluido = document.getElementById("iva_incluido").value
                dta.descuento = document.getElementById("descuento").value
                dta.document_blob = new Blob([csv])
dta.euro_dolar = document.getElementById("euro_dolar").value
                dta.dolar_peso = document.getElementById("dolar_peso").value
                dta.territorio = document.getElementById("territorio").options[document.getElementById("territorio").selectedIndex].value;
                $.ajax({
                    type: "POST",
                    url: "/executeLoadFile",
                    async: false,
                    dataType: 'json',
                    contentType: 'application/json',
                    data: JSON.stringify(dta)
                }).done(function(status){
                    str = "";
                    for(var i = 0; i < status.no_subido.length; i++){
                        str += status.no_subido[i] + '\n'
                    }
                    document.getElementById("obr").innerText += str 
                    $('.hover_bkgr_fricc').show();
                });
                return false

            }
        }
    }
    $('.popupCloseButton').click(function(){
        $('.hover_bkgr_fricc').hide();
        location.reload()
    });
    ( function ( $ ) {
        "use strict";
        jQuery( '#vmap' ).vectorMap( {
            map: 'world_en',
            backgroundColor: null,
            color: '#ffffff',
            hoverOpacity: 0.7,
            selectedColor: '#1de9b6',
            enableZoom: true,
            showTooltip: true,
            values: sample_data,
            scaleColors: [ '#1de9b6', '#03a9f5' ],
            normalizeFunction: 'polynomial'
        } );
    } )( jQuery );
</script>

